{
  "manifest_version": 2,

  "name": "Rabbit Hole",
  "description": "Turn the Internet into a riddle and find your way down the rabbit hole.",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "storage",
    "unlimitedStorage",
    "https://en.wikipedia.org/",
    "http://en.wikipedia.org/",
    "http://localhost/*",
    "http://127.0.0.1/*",
    "tabs"
  ],
  "background": {
    "scripts": [ "jquery-2.1.3.js", "background.js"],
  "persistent": false
  },
  "content_scripts": [{
//    "matches": ["http://en.wikipedia.org/*", "https://en.wikipedia.org/*"],
    "matches": ["*://*/wiki/*"],
    "css":["style.css"],
    "js": ["jquery-2.1.3.js", "contentscript.js"],
    "run_at": "document_end"
  }],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Click Here!"
  },
  "web_accessible_resources":[
    "tray.html",
    "img/*"
  ]
}